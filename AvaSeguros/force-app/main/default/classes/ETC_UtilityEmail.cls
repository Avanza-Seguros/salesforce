    public static OrgWideEmailAddress getOrgWideEmailAddress(String Name){
        return [SELECT Id, Address, DisplayName FROM OrgWideEmailAddress WHERE DisplayName =: Name LIMIT 1];
    }
   
    public static List<Messaging.EmailFileAttachment> getFiles(Id recordId){
        List<Messaging.EmailFileAttachment> emailAttachList = new List<Messaging.EmailFileAttachment>();
        List<ContentVersion> listContentVersion = ETC_UtilityFile.getFiles(recordId);
            for (ContentVersion file : listContentVersion){
                emailAttachList.add(setEmailFileAttachment(file));
            }
            return emailAttachList;
    }
    public static Messaging.EmailFileAttachment setEmailFileAttachment(ContentVersion file){
        Messaging.EmailFileAttachment emailFileAttachment = new Messaging.EmailFileAttachment();
        Blob fileData = file.VersionData;
        emailFileAttachment.setFilename(file.Title+'.'+file.FileExtension);
        emailFileAttachment.setBody(fileData);
        return emailFileAttachment;
    }

    

    public static void sendEmailTemplateWithFiles(String templateName, List<String> listEmails,String personId ,String recordId){
        
        Messaging.SingleEmailMessage email = getSingleEmailTemplate(templateName, listEmails, personId, recordId);
        List<Messaging.EmailFileAttachment> emailFileAttList = getFiles(recordId);
        if (emailFileAttList.size() > 0){
            email.setFileAttachments(emailFileAttList);
        }
        Messaging.sendEmail(new Messaging.SingleEmailMessage[] { email });
    }

    private static Messaging.SingleEmailMessage getSingleEmailTemplate(String templateName, List<String> listEmails, String personId ,String recordId){
        EmailMetadata__mdt mdt = ETC_UtilityMetadata.getCustomMetadataRecord('Default');

        EmailTemplate template = getEmailTemplate(templateName)[0];
        Messaging.SingleEmailMessage email = Messaging.renderStoredEmailTemplate(template.Id, personId, recordId);
        email.setTemplateId(template.Id);
        // email.setTargetObjectId(personId); 
        email.setToAddresses(listEmails);
        // email.setOrgWideEmailAddressId(param0);
        email.setSenderDisplayName(mdt.DisplayName__c);
        // email.setSubject(template.Subject);
        // email.setHtmlBody(template.HtmlValue);
        // email.setWhatId(recordId);
        email.setSubject(email.getSubject());
        email.setHtmlBody(email.getHtmlBody());

        email.setSaveAsActivity(false);
        email.setUseSignature(false);
        email.setBccSender(false);
        return email;
    }

    public static List<EmailTemplate> getEmailTemplate(String templateName){
        return [SELECT Id,HtmlValue,Subject, DeveloperName FROM EmailTemplate WHERE DeveloperName =: templateName];
    }
   
